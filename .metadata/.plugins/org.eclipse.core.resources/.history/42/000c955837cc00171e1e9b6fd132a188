package lt.and.pac.treerecursion;

import java.util.ArrayList;
import java.util.List;

public class Node {

	private int name;
	List<Node> children;
	
	public Node(int nameIn){
		this.name = nameIn;
		this.children = new ArrayList();
	}
	
	public int getName(){
		return name;
	}
	
	public List getChildren(){
		return children;
	}
	
}
